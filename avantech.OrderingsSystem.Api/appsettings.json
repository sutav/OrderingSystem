{
  "ConnectionStrings": {
    "OrderingSystem": "Server=W10L13210721157;Database=OrderingSystem;Trusted_Connection=True;TrustServerCertificate=True;",
    "ServiceBus": "key_vault",
    "Redis": "key_vault"
  },
 
  
  "Serilog": {
    "Using": [
      "Serilog.Sinks.File",
      "Serilog.Sinks.AzureLogAnalytics",
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "Microsoft.AspNetCore": "Warning"
      }
    },
    "WriteTo": [      
      {
        "Name": "File",
        "Args": {
          "path": "C:/Logs/avantech.OrderingSystem/OrderingSystem.Api.log",
          "rollingInterval": "Day",
          "outputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss.fff} {Level:u3} {correlationId}] - {SourceContext} - {Message}\n" // Show CorrelationID 
        }
      }
    ],
    "Enrich": [ "FromLogContext" ],
    "Properties": {
      "Application": "VehicleApi"
    }
  },
  "AllowedHosts": "*",
  "ApplicationInsights": {
    "ConnectionString": "" // Set value in Octopus
  }
 
}